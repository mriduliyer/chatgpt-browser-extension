{
  "name": "ChatGPT by Reyi",
  "version": "1.0",
  "manifest_version": 2,
  "description": "An extension powered by ChatGPT",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "ChatGPT by Reyi"
  },
  "permissions": [
    "tabs",
    "https://api.openai.com/*"
  ]
}
